<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.main.FaceGameController">
   <children>
      <Button fx:id="register_btn" layoutX="61.0" layoutY="251.0" mnemonicParsing="false" onAction="#registerBtn" text="얼굴 등록하기" />
      <ImageView fx:id="faceImg" fitHeight="211.0" fitWidth="247.0" layoutX="31.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="ok_btn" layoutX="185.0" layoutY="250.0" mnemonicParsing="false" onAction="#okBtn" text="확 인" />
   </children>
</AnchorPane>
