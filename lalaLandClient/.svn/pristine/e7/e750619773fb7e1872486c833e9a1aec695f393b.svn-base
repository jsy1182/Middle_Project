      <Label fx:id="la_date" layoutX="754.0" layoutY="68.0" prefHeight="15.0" prefWidth="125.0" />
      <TextArea fx:id="ta" layoutX="66.0" layoutY="99.0" prefHeight="154.0" prefWidth="817.0" />
      <Button fx:id="btn_insert" layoutX="295.0" layoutY="408.0" mnemonicParsing="false" onAction="#click_insert" prefHeight="27.0" prefWidth="76.0" text="등록하기" visible="false" />
      <RadioButton fx:id="radio" layoutX="278.0" layoutY="60.0" mnemonicParsing="false" text="비밀글" />
      <ImageView fitHeight="105.0" fitWidth="87.0" layoutX="4.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
