<AnchorPane fx:id="anchoPane" layoutX="-1.0" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Board.controller.ViewQAController">
