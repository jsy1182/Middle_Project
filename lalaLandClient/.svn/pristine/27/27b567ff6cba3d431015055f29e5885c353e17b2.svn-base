<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eventAdmin.controller.EventAdminAddController">
   <children>
      <Label layoutX="10.0" layoutY="14.0" text="이벤트 제목" />
      <Label layoutX="10.0" layoutY="41.0" text="이벤트 내용" />
      <Label layoutX="14.0" layoutY="287.0" text="이벤트 시작일" />
      <Label layoutX="14.0" layoutY="317.0" text="이벤트 종료일" />
      <Label layoutX="15.0" layoutY="345.0" text="이벤트 할인율" />
      <TextField fx:id="tfTitle" layoutX="90.0" layoutY="10.0" prefHeight="23.0" prefWidth="159.0" promptText="이벤트 제목 입력" />
      <TextArea fx:id="taContent" layoutX="10.0" layoutY="64.0" prefHeight="200.0" prefWidth="240.0" promptText="내용을 입력해 주세요" />
      <TextField fx:id="tfStart" layoutX="98.0" layoutY="283.0" prefHeight="23.0" prefWidth="151.0" promptText="18-01-27" />
      <TextField fx:id="tfEnd" layoutX="98.0" layoutY="312.0" prefHeight="23.0" prefWidth="151.0" promptText="19-01-27" />
      <TextField fx:id="tfDiscount" layoutX="98.0" layoutY="341.0" prefHeight="23.0" prefWidth="151.0" promptText="0.2" />
      <Button fx:id="btnAdd" layoutX="42.0" layoutY="372.0" mnemonicParsing="false" onAction="#btnOK" text="확인" />
      <Button fx:id="btnCancel" layoutX="161.0" layoutY="372.0" mnemonicParsing="false" onAction="#cancel" text="취소" />
   </children>
</AnchorPane>
