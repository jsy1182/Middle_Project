<AnchorPane prefHeight="465.0" prefWidth="401.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="member.controller.FixImformationEmpController">
   <children>
      <ImageView fitHeight="594.0" fitWidth="484.0" layoutX="-82.0" pickOnBounds="true" preserveRatio="true">
         <image>
