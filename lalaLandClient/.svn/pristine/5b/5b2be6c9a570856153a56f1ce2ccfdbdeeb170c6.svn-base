<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.main.MiniGameMainController">
   <children>
      <Button fx:id="btnMemory" layoutX="18.0" layoutY="36.0" mnemonicParsing="false" onAction="#playMemoryGame" prefHeight="23.0" prefWidth="105.0" text="기억력 게임" />
      <Button fx:id="btnRoulette" layoutX="190.0" layoutY="36.0" mnemonicParsing="false" onAction="#playRoulette" text="행맨" />
      <ImageView fitHeight="179.0" fitWidth="112.0" layoutX="22.0" layoutY="72.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@unnamed.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="179.0" fitWidth="100.0" layoutX="160.0" layoutY="72.0">
         <image>
            <Image url="@hangman.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
